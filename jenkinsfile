node {
    stage('Checkout') {
        checkout([$class: 'GitSCM', branches: [[name: 'develop']], userRemoteConfigs: [[url: 'https://github.com/Discover-git/Mygitpipeline.git']]])
    }

    stage('Pull to Folder') {
               def targetFolder = 'my-folder'
                dir(targetFolder) {
            sh 'git pull origin develop'
        }
    }
}
